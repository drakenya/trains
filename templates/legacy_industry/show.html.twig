{% extends 'base.html.twig' %}

{% block title %}LegacyIndustry{% endblock %}

{% block body %}
    <h1>LegacyIndustry</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ legacy_industry.id }}</td>
            </tr>
            <tr>
                <th>Era</th>
                <td>{{ legacy_industry.era }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ legacy_industry.name }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ legacy_industry.city }}</td>
            </tr>
            <tr>
                <th>State</th>
                <td>{{ legacy_industry.state }}</td>
            </tr>
            <tr>
                <th>ReportingMarks</th>
                <td>{{ legacy_industry.reportingMarks }}</td>
            </tr>
            <tr>
                <th>ShipReceive</th>
                <td>{{ legacy_industry.shipReceive }}</td>
            </tr>
            <tr>
                <th>Commodity</th>
                <td>{{ legacy_industry.commodity }}</td>
            </tr>
            <tr>
                <th>Stcc</th>
                <td>{{ legacy_industry.stcc }}</td>
            </tr>
            <tr>
                <th>Reciprocal</th>
                <td>{{ legacy_industry.reciprocal }}</td>
            </tr>
            <tr>
                <th>Source</th>
                <td>{{ legacy_industry.source }}</td>
            </tr>
            <tr>
                <th>ExternalSource</th>
                <td>{{ legacy_industry.externalSource }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('legacy_industry_index') }}">back to list</a>

    {% if is_granted('ROLE_ADMIN') %}<a href="{{ path('legacy_industry_edit', {'id': legacy_industry.id}) }}">edit</a>{% endif %}

    {% if is_granted('ROLE_ADMIN') %}{{ include('legacy_industry/_delete_form.html.twig') }}{% endif %}
{% endblock %}